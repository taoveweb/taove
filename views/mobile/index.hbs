{{#extend "mStyle"}}

    <style>
        html, body {
            position: relative;
            height: 100%;
        }

        body {
            background: #fff;
            font-size: 14px;
            color: #000;
            margin: 0;
            padding: 0;
        }

        .wrap {
            position: relative;
            width: 100%;
            height: 100%;
        }


        .menu .home{
            background: url("data:image/png;base64,") no-repeat center;
        }
        .menu .explorer{
            background: url("data:image/png;base64,") no-repeat center;

        }
        .menu .sk{
            background: url("data:image/png;base64,") no-repeat center;

        }
        .menu .message{
            background: url("data:image/png;base64,") no-repeat center;

        }
        .menu .my{
            background: url("data:image/png;base64,") no-repeat center;

        }

        .menu a.home,
        .menu a.explorer,
        .menu a.sk,
        .menu a.message,
        .menu a.my{
            background-size:auto 70%;
        }
        .menu a.sk{
            background-size:auto 90%;
        }


        .menu{
            position: absolute;
            bottom: 0;
            width: 100%;
            height: 1.2rem;
            display: flex;
            z-index: 10;
            flex-direction: row;
            background: #222729;
        }
        .menu a{
            flex: 1;
            line-height: 1.2rem;
            text-align: center;
            background: #222729;
            text-decoration: none;
            font-size: 0.3rem;
            color: #fff;
        }
        .menu a.active{
            background-color: #000;
        }


    </style>

{{/extend}}
<div class="wrap">
    <div class="menu">
        <a class="active home" href="http://www.taove.com/m/albums"></a>
