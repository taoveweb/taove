                }
                This.parent().find('.like-num').html(likenum);
                $.post('/m', {name: name}, function (data) {
                    if (!data.sucess) {
                        /* This.parent().find('.bigLike').toggleClass('active');
                         likenum--;
                         This.parent().find('.like-num').html(likenum);*/
                    }
                })
            }
        });


    </script>
